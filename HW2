LIBNAME FF "/home/farazk_9190/FF"; 
 
title 'Faraz Farooq Lab 3 PSTAT 130 Problem 1'; 
PROC PRINT DATA= FF.gradegsv1 NOOBS N; 
WHERE (QFinal BETWEEN 85 and 95) OR (HWFinal GE 95); 
VAR QFinal HWFinal Midterm Final FinalScore; 
RUN; 
 
DATA FF.gradegsv2; 
SET FF.gradegsv1; 
LENGTH Grade $ 2.; 
IF  
FinalScore <60 THEN DO; 
Grade = 'F'; 
Letter= 'F'; 
END; 
ELSE IF FinalScore < 63 THEN DO; 
Grade = 'D-'; 
Letter = 'D'; 
END; 
ELSE IF FinalScore < 67 THEN DO; 
Grade = 'D'; 
Letter = 'D'; 
END; 
ELSE IF FinalScore < 70 THEN DO; 
Grade = 'D+'; 
Letter = 'D'; 
END; 
ELSE IF FinalScore < 73 THEN DO; 
Grade = 'C-'; 
Letter = 'C'; 
END; 
ELSE IF FinalScore < 77 THEN DO; 
Grade = 'C'; 
Letter = 'C'; 
END; 
ELSE IF FinalScore < 80 THEN DO; 
Grade = 'C+'; 
Letter = 'C'; 
END; 
ELSE IF FinalScore < 83 THEN DO; 
Grade = 'B-'; 
Letter = 'B'; 
END; 
ELSE IF FinalScore < 87 THEN DO; 
Grade = 'B'; 
Letter = 'B'; 
END; 
ELSE IF FinalScore < 90 THEN DO; 
Grade = 'B+'; 
Letter = 'B'; 
END; 
ELSE IF FinalScore < 93 THEN DO; 
Grade = 'A-'; 
Letter= 'A'; 
END; 
ELSE IF FinalScore < 97 THEN DO; 
Grade = 'A'; 
Letter= 'A'; 
END; 
ELSE IF FinalScore < 101 THEN DO; 
Grade = 'A+'; 
Letter= 'A'; 
END; 
RUN; 
title 'Problem 2A'; 
PROC PRINT DATA= FF.gradegsv2 NOOBS N; 
WHERE Grade = 'A+'; 
VAR  QFINAL HWFINAL Midterm Final FinalScore Grade; 
RUN; 
 
title 'Problem 2B'; 
PROC PRINT DATA= FF.gradegsv2 NOOBS N; 
WHERE LETTER = 'D' or LETTER= 'F'; 
VAR QFINAL HWFINAL Midterm Final FinalScore Letter; 
RUN; 
 
title 'Problem 3A'; 
PROC FREQ DATA=FF.gradegsv2; 
TABLES Grade Letter Grade*Letter/NOROW NOFREQ NOCUM; 
RUN; 
 
title 'Problem 3B'; 
PROC MEANS DATA= FF.gradegsv2 mean std Q1 Q3 MIN MAX; 
VAR QFinal HWFinal Midterm Final FinalScore; 
RUN;
